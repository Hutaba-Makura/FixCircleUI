{
    "manifest_version": 3,
    "name": "Fix Circle UI",
    "version": "1.0",
    "description": "コミケWebカタログのUIを改善します。",
    "content_scripts": [
      {
        "matches": ["https://webcatalog-free.circle.ms/Circle/List", "https://webcatalog-free.circle.ms/User/Favorites"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ],
    "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}